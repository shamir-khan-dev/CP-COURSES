# Imports



def twoSum(nums, target):
    """
    :type nums: List[int]
    :type target: int
    :rtype: List[int]
        """
    for x in range(len(nums)):
        for y in range(len(nums)):
            if(nums[x] + nums[y+1] == target):
                    return [x, y]

print(twoSum([2,5,5,11],10))